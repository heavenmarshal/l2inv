NLEQ_FLAG = `echo 'library(nleqslv); cat(system.file(package="nleqslv",mustWork=TRUE))'\
| "${R_HOME}/bin/R" --vanilla --slave`/libs/x64/nleqslv.dll
PKG_CFLAGS = -DRPRINT -I include -fopenmp  ## -UNDEBUG
PKG_LIBS = ${LAPACK_LIBS} ${BLAS_LIBS} ${FLIBS}  "${NLEQ_FLAG}"
